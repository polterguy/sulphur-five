
/*
 * Invoked when an image file is clicked.
 *
 * Expects [image] being full path to image.
 */





/*
 * Simply showing image in modal widget.
 */
p5.web.get-location
p5.html.url-encode:x:/../*/image?value
create-widget
  parent:sulphur-file-preview-content
  element:img
  alt:Preview of image
  onclick
  src:{0}?file={1}
    :x:/@p5.web.get-location?value
    :x:/@p5.html.url-encode?value
